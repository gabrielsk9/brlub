// Executa gravação do Log de Uso da rotina
U_BFCFGM01()

dbSelectArea("SA1")
dbSetOrder(1)
If dbSeek(xFilial("SA1")+cCliente+cLoja)
	cCEP := SA1->A1_CEP
	
	If SA1->A1_ROTA <> " "
		cRota := SA1->A1_ROTA
	Endif
	
	dbSelectArea("PAB")
	dbSetOrder(1)
	If dbSeek(xFilial("PAB")+cCEP)
		cPrazo:= PAB->PAB_PRAZO
	Endif
Endif
RestArea(aAreaOld)

Return(cPrazo)
