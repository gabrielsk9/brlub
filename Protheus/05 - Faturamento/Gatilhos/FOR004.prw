Local 	aAreaOld	:= GetArea()
Local 	cCliente 	:= M->C5_CLIENTE
Local 	cLoja    	:= M->C5_LOJACLI
Local 	cPrazo   	:= " "
Local 	cCEP		:= " "
Local 	cRota		:= " "

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
